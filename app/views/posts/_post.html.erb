<tr>
  <td><%= post.published %></td>
  <td><%= post.content %></td>
  <td><%= link_to 'Show', post %></td>
  <td><%= link_to 'Edit', edit_post_path(post) %></td>
  <% if policy(post).destroy? %>
    <td><%= link_to 'Destroy', post, method: :delete, data: { donfirm: 'Are you sure?'} %></td>
  <% end %>
</tr>