<h1>Tracks</h1>
 
 <p>
<div class="container">
<table class="table">
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
 
  <% @tracks.each do |track| %>
    <tr>
      <td><%= track.title %></td>
      <td><%= track.description %></td>
      <td><%= link_to 'Show Activities', track_path(track) %></td>
      <td><%= link_to 'Edit', edit_track_path(track) %></td>
      <td><%= link_to 'Destroy', track_path(track),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td> 
    </tr>
  <% end %>
</table>
<%= link_to 'New', new_track_path  %>
</div>
